{
  "name": "mojoin-core",
  "version": "0.0.6",
  "description": "Lightweight Business Intelligence (BI) Library: Load distributed and microservice datasources into a SQL RDBMS and query them.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ingo-eichhorst/mojoin-core"
  },
  "scripts": {
    "start": "node lib/index.js",
    "test": "jest . --config jest.config.json",
    "lint": "eslint .",
    "test-ci": "jest . --config jest.config.json && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test-codacy-coverage": "jest . --config jest.config.json && cat ./coverage/lcov.info | codacy-coverage"
  },
  "keywords": [
    "bi",
    "business intelligence",
    "reporting",
    "microservice",
    "distributed datasources",
    "rest",
    "mongodb"
  ],
  "author": "Ingo Eichhorst",
  "license": "MIT",
  "dependencies": {
    "axios": "0.18.0",
    "debug": "4.1.0",
    "ejson": "2.2.0",
    "generate-schema": "2.6.0",
    "jsonschema": "1.2.4",
    "mongodb": "3.1.3",
    "node-ssh": "5.1.2",
    "sequelize": "4.41.0",
    "sequelizer": "1.1.3",
    "sqlite3": "4.0.2"
  },
  "devDependencies": {
    "codacy-coverage": "3.0.0",
    "coveralls": "3.0.2",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jest": "21.21.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "jest": "23.5.0",
    "jest-junit": "5.1.0",
    "koa": "2.5.3",
    "nock": "9.6.1"
  }
}
